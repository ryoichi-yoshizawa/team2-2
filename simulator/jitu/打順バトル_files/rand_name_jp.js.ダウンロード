// データ
var sei_names1 = ["あ", "い", "う", "え", "お"];
var sei_names2 = ["あ", "い", "う", "え", "お"];
var mei_names1 = ["あ", "い", "う", "え", "お"];
var mei_names2 = ["あ", "い", "う", "え", "お"];



// メソッド
function getRandomName(){
	var sei1 = sei_names1[Math.floor(Math.random() * sei_names1.length)];
	var sei2 = sei_names2[Math.floor(Math.random() * sei_names2.length)];
	var mei1 = mei_names1[Math.floor(Math.random() * mei_names1.length)];
	var mei2 = mei_names2[Math.floor(Math.random() * mei_names2.length)];
	return sei1 + sei2 + " " + mei1 + mei2;
}

